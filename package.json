{
  "name": "@vu/vu-om-websdk",
  "version": "2.4.0",
  "description": "VU Onboarding Management Web SDK",
  "main": "dist/vu-idcard.umd.js",
  "module": "dist/vu-idcard.es.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\"",
    "build:dev": "npm run clean && cross-env DEBUG_ALLOW_VIRTUAL=true rollup -c && cross-env DEBUG_ALLOW_VIRTUAL=true node scripts/minify-externals.js",
    "build": "npm run clean && cross-env DEBUG_ALLOW_VIRTUAL=false rollup -c && cross-env DEBUG_ALLOW_VIRTUAL=false node scripts/minify-externals.js",
    "postinstall": "echo Running SDK postinstall && node dist/scripts/copy-files.js",
    "generate-manifest": "node virtualcam/generate-manifest.mjs",
    "prepare-worker": "node virtualcam/prepare-worker.mjs"
  },
  "publishConfig": {
    "registry": "https://git.clients.vusecurity.com/api/v4/projects/143/packages/npm/"
  },
  "author": "VU Inc.",
  "license": "COMMERCIAL",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@eslint/js": "^9.11.1",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-dynamic-import-vars": "^2.1.3",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.11.1",
    "globals": "^15.12.0",
    "rollup": "^4.52.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-obfuscator": "^1.1.0",
    "rollup-plugin-visualizer": "^5.12.0"
  }
}
